{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "items": {
        "properties": {
            "dimensions": {
                "properties": {
                    "height": {
                        "type": "number"
                    },
                    "length": {
                        "type": "number"
                    },
                    "width": {
                        "type": "number"
                    }
                },
                "required": [
                    "length",
                    "width",
                    "height"
                ],
                "type": "object"
            },
            "id": {
                "description": "The unique identifier for a product",
                "type": "number"
            },
            "name": {
                "type": "string"
            },
            "price": {
                "exclusiveMinimum": true,
                "minimum": 0,
                "type": "number"
            },
            "tags": {
                "items": {
                    "type": "string"
                },
                "minItems": 1,
                "type": "array",
                "uniqueItems": true
            },
            "warehouseLocation": {
                "$ref": "http://json-schema.org/geo",
                "description": "Coordinates of the warehouse with the product"
            }
        },
        "required": [
            "id",
            "name",
            "price"
        ],
        "title": "Product",
        "type": "object"
    },
    "title": "Product set",
    "type": "array"
}
